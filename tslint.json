{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "rulesDirectory": [],
  "linterOptions": {
    "project": "./lib"
  },
  "rules": {
    "max-line-length": {
      "options": [120]
    },
    "new-parens": true,
    "no-arg": false,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "no-console": false
  },
  "jsRules": {}
}
